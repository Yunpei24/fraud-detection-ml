{
  "id": null,
  "uid": "ml_model_performance",
  "title": "ðŸ¤– ML Model Performance Monitor",
  "tags": ["fraud-detection", "ml", "models", "performance"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "ðŸ“Š Model AUC-ROC Scores",
      "type": "timeseries",
      "targets": [
        {
          "expr": "fraud_model_auc_roc",
          "legendFormat": "{{model_name}} v{{model_version}}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "fieldConfig": {
        "defaults": {
          "min": 0.5,
          "max": 1.0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 0.90, "color": "yellow"},
              {"value": 0.95, "color": "green"}
            ]
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["lastNotNull", "mean"]
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 2,
      "title": "ðŸŽ¯ Current Model Metrics",
      "type": "stat",
      "targets": [
        {
          "expr": "fraud_model_auc_roc{model_name=\"random_forest\"}",
          "legendFormat": "AUC",
          "refId": "A"
        },
        {
          "expr": "fraud_model_recall{model_name=\"random_forest\"}",
          "legendFormat": "Recall",
          "refId": "B"
        },
        {
          "expr": "fraud_model_precision{model_name=\"random_forest\"}",
          "legendFormat": "Precision",
          "refId": "C"
        },
        {
          "expr": "fraud_model_f1_score{model_name=\"random_forest\"}",
          "legendFormat": "F1 Score",
          "refId": "D"
        }
      ],
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 0.85, "color": "yellow"},
              {"value": 0.90, "color": "green"}
            ]
          },
          "decimals": 4
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "justifyMode": "auto",
        "textMode": "value_and_name",
        "orientation": "auto"
      }
    },
    {
      "id": 3,
      "title": "â±ï¸ Training Duration by Model",
      "type": "barchart",
      "targets": [
        {
          "expr": "fraud_training_duration_seconds_sum / fraud_training_duration_seconds_count",
          "legendFormat": "{{model_name}} - {{stage}}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showValue": "always"
      }
    },
    {
      "id": 4,
      "title": "ðŸ“ˆ Model Precision Over Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "fraud_model_precision",
          "legendFormat": "{{model_name}}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "fieldConfig": {
        "defaults": {
          "min": 0.5,
          "max": 1.0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 5,
      "title": "ðŸ” Model Recall Over Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "fraud_model_recall",
          "legendFormat": "{{model_name}}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "fieldConfig": {
        "defaults": {
          "min": 0.5,
          "max": 1.0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 6,
      "title": "ðŸ‹ï¸ Training Runs - Success vs Failure",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(fraud_training_runs_total{status=\"success\"})",
          "legendFormat": "Success",
          "refId": "A"
        },
        {
          "expr": "sum(fraud_training_runs_total{status=\"failed\"})",
          "legendFormat": "Failed",
          "refId": "B"
        }
      ],
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 16},
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"}
            ]
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Failed"},
            "properties": [
              {
                "id": "color",
                "value": {"mode": "fixed", "fixedColor": "red"}
              }
            ]
          }
        ]
      },
      "options": {
        "colorMode": "background"
      }
    },
    {
      "id": 7,
      "title": "ðŸ§ª Hyperparameter Tuning Trials",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(fraud_hyperparameter_trials_total[5m])",
          "legendFormat": "{{model_name}}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 16},
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "lineWidth": 1,
            "fillOpacity": 20
          }
        }
      }
    },
    {
      "id": 8,
      "title": "ðŸ’¾ Training Memory Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "fraud_training_memory_usage_mb",
          "legendFormat": "{{model_name}}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 6, "w": 6, "x": 12, "y": 16},
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 9,
      "title": "âš¡ Training CPU Usage",
      "type": "gauge",
      "targets": [
        {
          "expr": "fraud_training_cpu_percent",
          "legendFormat": "{{model_name}}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 6, "w": 6, "x": 18, "y": 16},
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 70, "color": "yellow"},
              {"value": 90, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 10,
      "title": "ðŸ“Š MLflow Integration Status",
      "type": "stat",
      "targets": [
        {
          "expr": "fraud_mlflow_models_registered_total",
          "legendFormat": "Models Registered",
          "refId": "A"
        },
        {
          "expr": "sum(fraud_mlflow_runs_total{status=\"finished\"})",
          "legendFormat": "Runs Completed",
          "refId": "B"
        },
        {
          "expr": "fraud_mlflow_experiments_total",
          "legendFormat": "Experiments",
          "refId": "C"
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 22},
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "orientation": "horizontal"
      }
    },
    {
      "id": 11,
      "title": "â° Last Successful Training",
      "type": "stat",
      "targets": [
        {
          "expr": "(time() - max(timestamp(fraud_training_runs_total{status=\"success\"})))",
          "legendFormat": "Time Since Last Success",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 6, "w": 6, "x": 12, "y": 22},
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 3600, "color": "yellow"},
              {"value": 86400, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "colorMode": "background"
      }
    },
    {
      "id": 12,
      "title": "ðŸ“¦ Data Samples Processed",
      "type": "stat",
      "targets": [
        {
          "expr": "fraud_data_samples_processed_total{split=\"train\"}",
          "legendFormat": "Train",
          "refId": "A"
        },
        {
          "expr": "fraud_data_samples_processed_total{split=\"val\"}",
          "legendFormat": "Validation",
          "refId": "B"
        },
        {
          "expr": "fraud_data_samples_processed_total{split=\"test\"}",
          "legendFormat": "Test",
          "refId": "C"
        }
      ],
      "gridPos": {"h": 6, "w": 6, "x": 18, "y": 22},
      "options": {
        "colorMode": "none",
        "graphMode": "none",
        "orientation": "vertical"
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "model",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(fraud_model_auc_roc, model_name)",
        "refresh": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*"
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "datasource": "Prometheus",
        "enable": true,
        "expr": "changes(fraud_mlflow_models_registered_total[5m]) > 0",
        "iconColor": "green",
        "name": "Model Registered",
        "tagKeys": "model_name"
      },
      {
        "datasource": "Prometheus",
        "enable": true,
        "expr": "changes(fraud_training_runs_total{status=\"failed\"}[5m]) > 0",
        "iconColor": "red",
        "name": "Training Failed"
      }
    ]
  }
}

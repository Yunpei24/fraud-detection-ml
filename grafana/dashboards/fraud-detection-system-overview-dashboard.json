{
  "id": null,
  "title": "System Overview - Service Health & Infrastructure",
  "tags": [
    "fraud-detection",
    "system-overview",
    "health",
    "infrastructure"
  ],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Service Health Status",
      "type": "table",
      "targets": [
        {
          "expr": "up",
          "legendFormat": "{{job}}",
          "refId": "A",
          "instant": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                },
                "1": {
                  "text": "UP",
                  "color": "green"
                }
              },
              "type": "value"
            }
          ]
        }
      }
    },
    {
      "id": 2,
      "title": "Service Uptime",
      "type": "bargauge",
      "targets": [
        {
          "expr": "(time() - process_start_time_seconds) / 3600",
          "legendFormat": "{{job}} uptime (hours)",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "hours",
          "mappings": [
            {
              "options": {
                "from": 1,
                "to": 24,
                "result": {
                  "color": "green",
                  "text": "Good"
                }
              },
              "type": "range"
            },
            {
              "options": {
                "from": 0,
                "to": 1,
                "result": {
                  "color": "orange",
                  "text": "Recently Started"
                }
              },
              "type": "range"
            }
          ]
        }
      }
    },
